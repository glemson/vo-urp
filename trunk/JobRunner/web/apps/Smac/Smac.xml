<parameter>
<name>IMG_Z_SIZE</name>
<datatype>float</datatype>
<description>side of the third dimension (default assumes kpc as units).</description>
<defaultValue>2000</defaultValue>
</parameter>

<!-- TODO dependent on values 1 and 3 of  PART_DISTR -->
<parameter>
<name>FILL_CARROT</name>
<datatype>float</datatype>
