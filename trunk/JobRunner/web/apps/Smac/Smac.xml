<defaultValue>0</defaultValue>
</parameter>

<parameter xsi:type="ts:EnumeratedParameter" ID="PART_DISTR">
<name>PART_DISTR</name>
<datatype>integer</datatype>
<description>
Flag of particle distribution scheme on the image.
</description>
<validvalues>
<literal>
  <value>1</value>
  <title>SPH (flat 2D-map)</title>
</literal>
<literal>
  <value>2</value>
  <title>SPH (full-sky HealPix map)</title>
</literal>
<literal>
  <value>3</value>
  <title>TSC (flat 2D-map)</title>
</literal>
</validvalues>
</parameter>

<!-- TODO dependent on values 1 and 3 of  PART_DISTR -->
<parameter xsi:type="ts:EnumeratedParameter">
<name>IMG_XY_UNITS</name>
<datatype>string</datatype>
<description>Flag of the units of the side of image</description>
<defaultValue>2</defaultValue>
<validvalues ID="IMG_XY_UNITS_for_PART_DISTR_13">
<literal>
  <value>1</value>
  <title>arcmin</title>
</literal>
<literal>
  <value>2</value>
  <title>kpc</title>
  </literal>
</validvalues>

<dependency>
  <master>PART_DISTR</master>
  <choice>
    <choose>IMG_XY_UNITS_for_PART_DISTR_13</choose>
    <if>1</if>
  </choice>
  <choice>
    <choose>IMG_XY_UNITS_for_PART_DISTR_13</choose>
    <if>3</if>
  </choice>
</dependency>

</parameter>

<!-- TODO dependent on values 1 and 3 of  PART_DISTR -->
<parameter>
<name>IMG_XY_SIZE</name>
<datatype>float</datatype>
<description>The size of the image (in units given by IMG_XY_UNITS) (default value assummes kpc as units!)</description>
<defaultValue>2000</defaultValue>
</parameter>


<!-- TODO dependent on values 1 and 3 of  PART_DISTR -->
<parameter  xsi:type="ts:EnumeratedParameter">
<name>IMG_Z_UNITS</name>
<datatype>integer</datatype>
<description></description>
