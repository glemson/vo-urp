   <xsd:complexType name="AppliedAlgorithm">
      <xsd:annotation>
         <xsd:documentation>Represents the application of a particular algorithm in an experiment.

TODO decide whether we need this.</xsd:documentation>
         <xsd:appinfo>
            <vo-urp:identifier>_12_1_8e0028f_1224862111906_82091_852</vo-urp:identifier>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="base:MetadataObject">
            <xsd:sequence>
               <xsd:element name="algorithm" type="base:Reference">
                  <xsd:annotation>
                     <xsd:documentation>The actual algorithm that is applied</xsd:documentation>
