<parameter xsi:type="ts:EnumeratedParameter">
<name>CENTER_MOTION</name>
<datatype>integer</datatype>
<description>Flag of the cluster baricenter motion</description>
<isFixed>1</isFixed>
<defaultValue>0</defaultValue>
<validvalues>
<literal>
<value>0</value>
<title>disabled</title>
</literal>
<literal>
<value>1</value>
<title>enabled: at the particles velocity is sobstituted the cluster baricenter velocity</title>
</literal>
<literal>
<value>2</value>
<title>enabled: paricles velocity in the cluster baricenter rest frame</title>
</literal>
</validvalues>
</parameter>

<parameter xsi:type="ts:EnumeratedParameter">
<name>REMOVE_LOCAL_GROUP_VEL</name>
<datatype>integer</datatype>
<description>Flag to subtract the velocity of the local group </description>
<isFixed>1</isFixed>
<defaultValue>0</defaultValue>
<validvalues>
<literal>
<value>0</value>
<title>disabled</title>
</literal>
<literal>
<value>1</value>
<title>enabled</title>
</literal>
</validvalues>
</parameter>

<parameter xsi:type="ts:EnumeratedParameter" ID="CENTER">
<name>CENTER</name>
<datatype>integer</datatype>
<description>Flag of the definition of the center</description>
<defaultValue>2</defaultValue>
<validvalues>
<literal>
<value>0</value>
<title>barycenter of gas particles</title>
</literal>
<literal>
<value>1</value>
<title>selected by user</title>
</literal>
<literal>
<value>2</value>
<title>read by a file</title>
</literal>
</validvalues></parameter>

