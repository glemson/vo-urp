<datatype>float</datatype>
<description>Minimum Gamma Ray energy in GeV (for Gamma Maps)</description>
<defaultValue>1000</defaultValue>
</parameter>

<parameter xsi:type="ts:EnumeratedParameter">
<name>GIVE_MORE_INFO</name>
<datatype>integer</datatype>
<description></description>
<isFixed>1</isFixed>
<defaultValue>0</defaultValue>
<validvalues>
<literal>
<value>0</value>
<title>disabled</title>
</literal>
<literal>
<value>1</value>
<title>enabled</title>
</literal>
</validvalues>
</parameter>

<!-- TODO dependency on PART_DISTR 1 and 3  -->
<parameter xsi:type="ts:EnumeratedParameter">
<name>START_PROJECT</name>
<datatype>integer</datatype>
<description>First direction to project along</description>
<defaultValue>1</defaultValue>
<validvalues ID="START_PROJECT_for_PART_DISTR_13">
<literal>
<value>1</value>
<title>along z, xy plane</title>
</literal>
<literal>
<value>2</value>
<title>along y, xz plane</title>
</literal>
<literal>
<value>3</value>
<title>along x, yz plane</title>
</literal>
</validvalues>
<dependency>
  <master>PART_DISTR</master>
  <choice>
    <choose>START_PROJECT_for_PART_DISTR_13</choose>
    <if>1</if>
  </choice>
  <choice>
    <choose>START_PROJECT_for_PART_DISTR_13</choose>
    <if>3</if>
  </choice>
</dependency>
</parameter>

